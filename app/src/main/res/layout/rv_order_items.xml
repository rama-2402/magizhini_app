<?xml version="1.0" encoding="utf-8"?><!--<layout>--><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:id="@+id/itemView"    android:layout_marginTop="@dimen/material_size_4"    android:layout_marginBottom="@dimen/material_size_4"    android:layout_marginStart="@dimen/material_size_8"    android:layout_marginEnd="@dimen/material_size_8"    android:elevation="@dimen/material_size_4"    android:background="@drawable/shape_round_rectangle_8"    android:foreground="?android:attr/selectableItemBackground">    <TextView        android:id="@+id/tvDate"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:background="@drawable/shape_round_rectangle_8"        android:backgroundTint="@color/green_light"        android:elevation="@dimen/material_size_2"        android:padding="@dimen/material_size_4"        android:text="00/00/0000 - online - Rs: 00.00"        android:textColor="@color/green_base"        android:textSize="14sp"        android:textStyle="bold"        android:layout_marginTop="@dimen/material_size_8"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/tvOrderIdTxt"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="@string/order_id"        android:textSize="14sp"        android:textStyle="bold"        android:layout_marginStart="@dimen/material_size_8"        android:layout_marginTop="@dimen/material_size_8"        android:textColor="@color/gray700"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/tvDate" />    <TextView        android:id="@+id/tvOrderId"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/material_size_8"        android:text="12345678"        android:textColor="@color/gray700"        android:longClickable="true"        android:textSize="14sp"        android:textIsSelectable="true"        app:layout_constraintStart_toEndOf="@id/tvOrderIdTxt"        app:layout_constraintTop_toTopOf="@id/tvOrderIdTxt" />    <TextView        android:id="@+id/tvDeliveryPreferenceTxt"        android:layout_width="wrap_content"        android:textColor="@color/gray700"        android:layout_height="wrap_content"        android:text="Preference:  "        android:textSize="14sp"        android:textStyle="bold"        android:layout_marginStart="@dimen/material_size_8"        android:layout_marginTop="@dimen/material_size_4"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/tvOrderIdTxt" />    <TextView        android:id="@+id/tvDeliveryPreference"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/material_size_8"        android:text="Call on Delivery"        android:textColor="@color/gray700"        android:textSize="14sp"        android:textStyle="normal"        app:layout_constraintStart_toEndOf="@id/tvDeliveryPreferenceTxt"        app:layout_constraintTop_toTopOf="@id/tvDeliveryPreferenceTxt" />    <TextView        android:id="@+id/tvDeliveryNoteTxt"        android:layout_width="wrap_content"        android:textColor="@color/gray700"        android:layout_height="wrap_content"        android:text="Delivery Note: "        android:textSize="14sp"        android:textStyle="bold"        android:layout_marginStart="@dimen/material_size_8"        android:layout_marginTop="@dimen/material_size_4"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/tvDeliveryPreferenceTxt" />    <TextView        android:id="@+id/tvDeliveryNote"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/material_size_8"        android:singleLine="false"        android:text=""        android:textColor="@color/gray700"        android:textSize="14sp"        android:textStyle="normal"        android:layout_marginEnd="@dimen/material_size_16"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@id/tvDeliveryNoteTxt"        app:layout_constraintTop_toTopOf="@id/tvDeliveryNoteTxt" />    <TextView        android:id="@+id/tvAddressTxt"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/material_size_8"        android:layout_marginTop="@dimen/material_size_4"        android:singleLine="false"        android:text="Delivery Address: "        android:textColor="@color/gray700"        android:textSize="14sp"        android:textStyle="bold"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/tvDeliveryNote" />    <TextView        android:id="@+id/tvAddress"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginTop="@dimen/material_size_4"        android:singleLine="false"        android:text=""        android:textColor="@color/gray700"        android:textSize="14sp"        android:layout_marginStart="@dimen/material_size_8"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@id/tvAddressTxt"        app:layout_constraintTop_toBottomOf="@id/tvDeliveryNote" />    <View        android:id="@+id/view"        android:layout_width="match_parent"        android:layout_height="0.75dp"        android:background="@color/grey"        android:layout_marginTop="@dimen/material_size_8"        app:layout_constraintTop_toBottomOf="@id/tvAddress"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintEnd_toEndOf="parent"        />    <TextView        android:id="@+id/tvOrderStatus"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/material_size_8"        android:ellipsize="marquee"        android:focusable="true"        android:focusableInTouchMode="true"        android:marqueeRepeatLimit="marquee_forever"        android:scrollHorizontally="true"        android:singleLine="true"        android:text=""        android:textColor="@color/gray700"        android:textSize="16sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/view"        app:layout_constraintEnd_toStartOf="@id/btnSend"/>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/btnSend"        android:layout_width="125dp"        android:foreground="?attr/selectableItemBackground"        android:layout_height="wrap_content"        android:background="@drawable/bg_order_history_cancel"        app:layout_constraintTop_toBottomOf="@id/view"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintBottom_toBottomOf="parent">        <TextView            android:id="@+id/tvCancel"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:text="CANCELLED"            android:textColor="@color/white"            android:textStyle="bold"            android:paddingTop="@dimen/material_size_12"            android:paddingBottom="@dimen/material_size_12"            android:paddingStart="@dimen/material_size_12"            android:gravity="center"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            />    </androidx.constraintlayout.widget.ConstraintLayout>    <ru.nikartm.support.ImageBadgeView        android:id="@+id/ivCart"        android:layout_width="0dp"        android:layout_height="45dp"        android:layout_weight="1"        android:layout_gravity="end"        android:padding="4dp"        android:scaleType="center"        app:ibv_badgeValue="0"        app:ibv_badgeTextSize="12sp"        app:ibv_fixedBadgeRadius="8dp"        app:ibv_badgeTextStyle="bold"        app:ibv_badgeTextColor="#ffffff"        app:ibv_badgeColor="@color/matteRed"        app:ibv_badgeLimitValue="false"        app:layout_constraintBottom_toTopOf="@+id/tvDeliveryNote"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/tvDate"        android:background="?android:attr/selectableItemBackgroundBorderless"        android:src="@drawable/ic_cart"        android:layout_marginEnd="@dimen/material_size_16"        app:tint="@color/green_base"/></androidx.constraintlayout.widget.ConstraintLayout><!--    android:background="?android:attr/selectableItemBackgroundBorderless"--><!--</layout>-->